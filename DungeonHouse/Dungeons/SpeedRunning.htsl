    globalvar "d4_sr_time2" set %var.player/dungeontime%
    globalvar "d4_sr_name2" set %player.name%
    globalvar "d4_sr_mins2" set %var.player/d_minutes%
    globalvar "d4_sr_secs2" set %var.player/d_seconds%

    globalvar "sr_bc_place" set "&72nd"
    globalvar "sr_bc_name" set %player.name%
    globalvar "sr_bc_dungeon" set "&dThe Sith Tower"
    globalvar "sr_bc_mins" set %var.player/d_minutes%
    globalvar "sr_bc_secs" set %var.player/d_seconds%
    function "SpeedRunningBroadcast" true

    exit
}

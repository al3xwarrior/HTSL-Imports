/*
    * DUNGEON 4
*/
if (var "dungeon_id" == 4, var "dungeontime" < %var.global/d4_sr_time1%) {
    // Move Times Down
    globalvar "d4_sr_time3" set %var.global/d4_sr_time2%
    globalvar "d4_sr_name3" set %var.global/d4_sr_name2%
    globalvar "d4_sr_mins3" set %var.global/d4_sr_mins2%
    globalvar "d4_sr_secs3" set %var.global/d4_sr_secs2%

    globalvar "d4_sr_time2" set %var.global/d4_sr_time1%
    globalvar "d4_sr_name2" set %var.global/d4_sr_name1%
    globalvar "d4_sr_mins2" set %var.global/d4_sr_mins1%
    globalvar "d4_sr_secs2" set %var.global/d4_sr_secs1%

    globalvar "d4_sr_time1" set %var.player/dungeontime%
    globalvar "d4_sr_name1" set %player.name%
    globalvar "d4_sr_mins1" set %var.player/d_minutes%
    globalvar "d4_sr_secs1" set %var.player/d_seconds%

    globalvar "sr_bc_place" set "&61st"
    globalvar "sr_bc_name" set %player.name%
    globalvar "sr_bc_dungeon" set "&bThe Stolen Ship"
    globalvar "sr_bc_mins" set %var.player/d_minutes%
    globalvar "sr_bc_secs" set %var.player/d_seconds%
    function "SpeedRunningBroadcast" true

    exit
}
if (var "dungeon_id" == 4, var "dungeontime" < %var.global/d4_sr_time2%) {
